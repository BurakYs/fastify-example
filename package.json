{
    "name": "fastify-example",
    "version": "1.0.0",
    "main": "src/index.ts",
    "author": "@BurakYs",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "start": "cross-env NODE_ENV=production node --loader ./custom-loader.js --no-warnings .",
        "dev": "cross-env NODE_ENV=development node --loader ./custom-loader.js --no-warnings .",
        "dev:watch": "cross-env NODE_ENV=development nodemon",
        "typecheck": "tsc --noEmit",
        "lint": "biome lint",
        "format": "biome check --write",
        "test": "vitest run --reporter verbose",
        "check": "pnpm typecheck && pnpm format && pnpm test"
    },
    "dependencies": {
        "@fastify/autoload": "^6.3.1",
        "@fastify/rate-limit": "^10.3.0",
        "@fastify/swagger": "^9.5.1",
        "@fastify/swagger-ui": "^5.2.3",
        "dotenv": "^17.2.1",
        "fastify": "^5.4.0",
        "fastify-plugin": "^5.0.1",
        "fastify-type-provider-zod": "^5.1.0",
        "mongoose": "^8.17.0",
        "pino": "^9.7.0",
        "pino-pretty": "^13.1.1",
        "vitest": "^3.2.4",
        "zod": "^4.0.14"
    },
    "devDependencies": {
        "@biomejs/biome": "2.1.3",
        "@types/node": "^24.1.0",
        "cross-env": "^10.0.0",
        "nodemon": "^3.1.10"
    }
}
